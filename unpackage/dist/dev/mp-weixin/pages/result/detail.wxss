.page-wrap {
  height: 100vh;
  width: 100vw;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}
.page-wrap .evaluate-form-wrap {
  box-sizing: border-box;
  flex: 1;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}
.page-wrap .evaluate-form-wrap .content-wrap {
  flex: 1;
  padding: 8px 12px;
  overflow: auto;
}
.page-wrap .evaluate-form-wrap .content-wrap .title {
  font-size: 16px;
  font-weight: 600;
  margin-bottom: 12px;
}
.page-wrap .evaluate-form-wrap .content-wrap .score {
  width: 100%;
}
.page-wrap .evaluate-form-wrap .foot {
  border-top: 1px solid #dddddd;
  padding: 8px 12px;
  display: flex;
  justify-content: space-between;
  padding: 8px 0;
  background-color: #ffffff;
}
.page-wrap .evaluate-form-wrap .foot button {
  width: 46%;
  height: 40px;
  background-color: #5cd7aa;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #ffffff;
}
.page-wrap .evaluate-form-wrap .foot button.default {
  background-color: #ffffff;
  color: #000000;
}

.page-wrap {
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  padding: 16rpx;
  overflow-y: auto;
}
.page-wrap .content {
  box-sizing: border-box;
  width: 100%;
  height: 90%;
  background-color: #FFFFFF;
  border-radius: 16rpx;
  padding: 30rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
  overflow-x: hidden;
}
.page-wrap .content .tips {
  font-weight: bold;
  font-size: 45rpx;
  text-align: center;
  justify-content: center;
  align-items: center;
  display: flex;
  box-sizing: border-box;
  padding: 8rpx;
  width: 100%;
  height: 300rpx;
  background-color: #e7eaec;
  border-radius: 8px;
}
.page-wrap .content .deleteBtn {
  font-size: 16px;
  width: calc(100% - 68px);
  height: 32px;
  padding: 4px 0;
  border-radius: 4px;
  color: #ffffff;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: rgba(255, 0, 0, 0.5);
}
.page-wrap .content .result {
  margin-top: 8px;
  width: 100%;
}
.page-wrap .content .result .label {
  color: #999;
}
.page-wrap .content .result .value-wrap {
  display: flex;
  flex-direction: column;
}
.page-wrap .content .result .value-wrap .item-wrpa {
  display: flex;
  align-items: center;
  margin-top: 8px;
}
.page-wrap .content .score {
  margin-top: 24px;
  width: 100%;
}

