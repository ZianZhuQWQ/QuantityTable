.page-wrap {
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  padding: 16rpx;
  overflow-y: auto;
}
.page-wrap .content {
  box-sizing: border-box;
  width: 100%;
  height: 90%;
  background-color: #FFFFFF;
  border-radius: 16rpx;
  padding: 30rpx;
  display: flex;
  align-items: center;
  flex-direction: column;
  overflow-x: hidden;
}
.page-wrap .content .tips {
  font-weight: bold;
  font-size: 45rpx;
  text-align: center;
  justify-content: center;
  align-items: center;
  display: flex;
  box-sizing: border-box;
  padding: 8rpx;
  width: 100%;
  height: 300rpx;
  background-color: #e7eaec;
  border-radius: 8px;
}
.page-wrap .content .deleteBtn {
  font-size: 16px;
  width: calc(100% - 68px);
  height: 32px;
  padding: 4px 0;
  border-radius: 4px;
  color: #ffffff;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: rgba(255, 0, 0, 0.5);
}
.page-wrap .content .result {
  margin-top: 8px;
  width: 100%;
}
.page-wrap .content .result .label {
  color: #999;
}
.page-wrap .content .result .value-wrap {
  display: flex;
  flex-direction: column;
}
.page-wrap .content .result .value-wrap .item-wrpa {
  display: flex;
  align-items: center;
  margin-top: 8px;
}
.page-wrap .content .score {
  margin-top: 24px;
  width: 100%;
}

